{
    "manifest_version":  3,
    "name":  "Better YouTube Studio aka Boi\u0027s Club Studio",
    "version":  "v1.0.11",
    "description":  "Enhanced YouTube Studio with beautiful analytics dashboard and quick access menu.",
    "icons":  {
                  "16":  "icons/icon-16.png",
                  "48":  "icons/icon-48.png",
                  "128":  "icons/icon-128.png"
              },
    "permissions":  [
                        "storage"
                    ],
    "content_scripts":  [
                            {
                                "matches":  [
                                                "https://studio.youtube.com/*"
                                            ],
                                "js":  [
                                           "content.js"
                                       ],
                                "run_at":  "document_idle"
                            }
                        ],
    "web_accessible_resources":  [
                                     {
                                         "resources":  [
                                                           "icons/*.svg",
                                                           "icons/*.png",
                                                           "inject.js",
                                                           "data/queries.json",
                                                           "styles/main.css"
                                                       ],
                                         "matches":  [
                                                         "https://studio.youtube.com/*"
                                                     ]
                                     }
                                 ]
}